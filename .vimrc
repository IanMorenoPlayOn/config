:set relativenumber
:set number
:nnoremap C-d C-dzz
:nnoremap C-u C-uzz
:synatx on
:set noncompatible
:set ruler
:highlight LineNr term=NONE cterm=NONE ctermfg=DarkGrey ctermbg=NONE gui=NONE guifg=DarkGrey guibg=NONE
:set tabstop=4
:set shiftwidth=4
:set autoindent
:set smartindent
:set showmatch
:set incsearch
:set nolist
filetype plugin indent on
